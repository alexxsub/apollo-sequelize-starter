{
  "name": "apollo-sequelize",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "yarn dbcreate && yarn migrate && yarn seed ",
    "dbcreate": "node_modules/.bin/sequelize db:create --config ./server/config/config.json",
    "migrategen": "node_modules/.bin/sequelize migration:generate --name temp  --config ./server/config/config.json --seeders-path ./server/seeders --migrations-path ./server/migrations --models-path ./server/models",
    "migrate": "node_modules/.bin/sequelize db:migrate --config ./server/config/config.json --seeders-path ./server/seeders --migrations-path ./server/migrations --models-path ./server/models",
    "unmigrate": "node_modules/.bin/sequelize db:migrate:undo:all --config ./server/config/config.json --seeders-path ./server/seeders --migrations-path ./server/migrations --models-path ./server/models",
    "seed": "node_modules/.bin/sequelize db:seed:all --config ./server/config/config.json --seeders-path ./server/seeders --migrations-path ./server/migrations --models-path ./server/models",
    "unseed": "node_modules/.bin/sequelize db:seed:undo:all --config ./server/config/config.json --seeders-path ./server/seeders --migrations-path ./server/migrations --models-path ./server/models",
    "start": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server-express": "^2.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql-tools": "^7.0.2",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.6"
  }
}
